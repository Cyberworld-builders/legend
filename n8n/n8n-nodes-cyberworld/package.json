{
  "name": "n8n-nodes-cyberworld",
  "version": "0.1.0",
  "description": "CyberWorld Builders custom nodes for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "tsc --noEmit"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/CyberWorldTest/CyberWorldTest.node.js",
      "dist/nodes/GetNextUnprocessedTranscript/GetNextUnprocessedTranscript.node.js",
      "dist/nodes/TranscriptCleanupAgent/TranscriptCleanupAgent.node.js"
    ],
    "credentials": [
      "dist/credentials/CyberWorldApi.credentials.js",
      "dist/credentials/OpenAiApi.credentials.js"
    ]
  },
  "devDependencies": {
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
